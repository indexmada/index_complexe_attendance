<odoo>
  <data>
    <record model="ir.ui.view" id="hr_work_entry_type_view_form">
      <field name="name">hr.work.entry.type.form.inherit</field>
      <field name="model">hr.work.entry.type</field>
      <field name="inherit_id" ref="hr_work_entry.hr_work_entry_type_view_form"/>
      <field name="arch" type="xml">
        <xpath expr="//group[@name='main_group']" position="after">
          <group>
            <group string="Majoration">
              <field name="sunday_maj"/>
              <field name="holiday_maj"/>
              <field name="night_maj"/>
            </group>
          </group>
        </xpath>  
      </field>
    </record>

  </data>
</odoo>